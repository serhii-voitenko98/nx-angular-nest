{
  "name": "nx-angular-nest",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "frontend-client:start": "ng serve frontend-client",
    "frontend-client:build": "ng build frontend-client",
    "frontend-client:test": "ng test frontend-client",
    "frontend-client:build:ssr": "ng build frontend-client --prod && ng run frontend-client:server:production",
    "frontend-client:serve:ssr": "node dist/frontend-client/server/main.js",
    "frontend-admin:start": "ng serve frontend-admin",
    "frontend-admin:build": "ng build frontend-admin",
    "frontend-admin:test": "ng test frontend-admin",
    "backend-api:start": "ng serve backend-api",
    "backend-api:build": "ng build backend-api",
    "backend-api:test": "ng test backend-api",
    "backend-api:entities": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.entities.json",
    "backend-api:migrations:run": "node_modules/.bin/tsc --project apps/backend/api/tsconfig.migrations.json && node_modules/.bin/ts-node node_modules/.bin/typeorm migration:run -t=false",
    "backend-api:migrations:revert": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:revert",
    "backend-api:migration:create": "node_modules/.bin/ts-node node_modules/.bin/typeorm migration:create -n",
    "db:start": "cp .env ./docker-compose && cd docker-compose && docker-compose -p nx-angular-nest up -d",
    "db:stop": "cd docker-compose && docker-compose stop && rm .env",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run frontend-client:serve-ssr",
    "serve:ssr": "node dist/frontend-client/server/main.js",
    "build:ssr": "ng build && ng run frontend-client:server",
    "prerender": "ng run frontend-client:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.1",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/localize": "^12.1.0",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/platform-server": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@nguniversal/express-engine": "^12.1.0",
    "@nrwl/angular": "12.7.1",
    "@nrwl/nest": "^12.7.1",
    "apollo-server-express": "^2.0.0",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.0.0-beta3",
    "express": "^4.15.2",
    "graphql": "^15.5.1",
    "graphql-tools": "^8.1.0",
    "graphql-type-json": "^0.3.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.37",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nguniversal/builders": "^12.1.0",
    "@nrwl/cli": "12.7.1",
    "@nrwl/cypress": "12.7.1",
    "@nrwl/eslint-plugin-nx": "12.7.1",
    "@nrwl/jest": "12.7.1",
    "@nrwl/linter": "12.7.1",
    "@nrwl/nest": "12.7.1",
    "@nrwl/node": "12.7.1",
    "@nrwl/tao": "12.7.1",
    "@nrwl/workspace": "12.7.1",
    "@types/express": "^4.17.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.4",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  }
}
